{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*.js",
      "src/**/*.jsx",
      "src/App.test.js", 
      "src/App.js",
      "src/index.js"    ],
    "exclude": [
      "node_modules"
    ]
  }